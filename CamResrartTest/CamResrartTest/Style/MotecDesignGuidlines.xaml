	<Style TargetType="Button" x:Key="trafficLight">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Height" Value="auto"/>
		<Setter Property="Width" Value="auto"/>
		<Setter Property="IsEnabled" Value="False"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Button}">
					<Grid Width="auto">
						<StackPanel Name="stcCanGreen" Orientation="Horizontal" HorizontalAlignment="Left" Width="auto" Visibility="Hidden">
							<Grid>
								<Ellipse Width="18" Height="18" Fill="Lime">
									<Ellipse.Effect>
										<DropShadowEffect BlurRadius="5" Color="lime" ShadowDepth="0"/>
									</Ellipse.Effect>
								</Ellipse>
								<Ellipse Width="18" Height="18" Stroke="#ff00de00">
									<Ellipse.Fill>
										<LinearGradientBrush StartPoint="0,1" EndPoint="0.5,0">
											<GradientStop Color="Lime" Offset="0.75" />
											<GradientStop Color="#ff00aa00" Offset="0.25" />
										</LinearGradientBrush>
									</Ellipse.Fill>
								</Ellipse>
							</Grid>
							<Grid Margin="3,0,0,0">
								<Ellipse Width="18" Height="18" Stroke="LightGray">
									<Ellipse.Fill>
										<LinearGradientBrush StartPoint="0,1" EndPoint="0.5,0">
											<GradientStop Color="LightGray" Offset="0.5" />
											<GradientStop Color="#ff797979" Offset="0.1" />
										</LinearGradientBrush>
									</Ellipse.Fill>
								</Ellipse>
							</Grid>
						</StackPanel>
						<StackPanel Name="stcCanRed" Orientation="Horizontal" HorizontalAlignment="Left" Width="auto">
							<Grid>
								<Ellipse Width="18" Height="18" Stroke="LightGray">
									<Ellipse.Fill>
										<LinearGradientBrush StartPoint="0,1" EndPoint="0.5,0">
											<GradientStop Color="LightGray" Offset="0.5" />
											<GradientStop Color="#ff797979" Offset="0.1" />
										</LinearGradientBrush>
									</Ellipse.Fill>
								</Ellipse>
							</Grid>
							<Grid Margin="3,0,0,0">
								<Ellipse Width="18" Height="18" Fill="Red" Stroke="#ff00de00">
									<Ellipse.Effect>
										<DropShadowEffect BlurRadius="5" Color="Red" ShadowDepth="0"/>
									</Ellipse.Effect>
								</Ellipse>
								<Ellipse Width="18" Height="18" Stroke="#ffde0000">
									<Ellipse.Fill>
										<LinearGradientBrush StartPoint="0,1" EndPoint="0.5,0">
											<GradientStop Color="red" Offset="0.75" />
											<GradientStop Color="#ffaa0000" Offset="0.25" />
										</LinearGradientBrush>
									</Ellipse.Fill>
								</Ellipse>
							</Grid>
						</StackPanel>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsEnabled" Value="True">
							<Setter TargetName="stcCanRed" Property="Visibility" 
								Value="Collapsed"/>
							<Setter TargetName="stcCanGreen" Property="Visibility" 
								Value="Visible"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="false">
							<Setter TargetName="stcCanRed" Property="Visibility" 
								Value="Visible"/>
							<Setter TargetName="stcCanGreen" Property="Visibility" 
								Value="Collapsed"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

