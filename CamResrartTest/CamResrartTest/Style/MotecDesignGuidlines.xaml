			<Grid.ColumnDefinitions>
				<ColumnDefinition />
				<ColumnDefinition Width="16" />
			</Grid.ColumnDefinitions>
			<Border
		  BorderBrush="{StaticResource stdBorder}"
		  CornerRadius="{StaticResource stdCornerRadius}"
		  BorderThickness="1,1,1,1"
		  Name="Border"
		  Background="{StaticResource stdBackGround}"
		  Grid.ColumnSpan="2" />
			<Path
		  Margin="0,0,3,0"
		  Data="M0,0L4,4 8,0z"
		  HorizontalAlignment="Center"
		  Fill="{StaticResource stdForeGround}"
		  Name="Arrow"
		  VerticalAlignment="Center"
		  Width="8"
		  Grid.Column="1" />
		</Grid>
		<ControlTemplate.Triggers>
			<Trigger Property="UIElement.IsMouseOver" Value="True">
				<Setter Property="Panel.Background" TargetName="Border" Value="{StaticResource ResourceKey=stdMouseOver}"/>
			</Trigger>
			<Trigger Property="ToggleButton.IsChecked" Value="True">
				<Setter Property="Panel.Background" TargetName="Border" Value="{StaticResource stdBackGround}"/>
				<Setter Property="Shape.Fill" TargetName="Arrow" Value="#FF8D979E"/>
			</Trigger>
			<Trigger Property="UIElement.IsEnabled" Value="False">
				<Setter Property="Panel.Background" TargetName="Border" Value="{StaticResource stdDisabled}"/>
				<Setter Property="Border.BorderBrush" TargetName="Border" Value="{StaticResource ResourceKey=stdClick}"/>
				<Setter Property="TextElement.Foreground" Value="{StaticResource stdDisabled}"/>
				<Setter Property="Shape.Fill" TargetName="Arrow" Value="#66FFFFFF"/>
			</Trigger>
		</ControlTemplate.Triggers>
	</ControlTemplate>
