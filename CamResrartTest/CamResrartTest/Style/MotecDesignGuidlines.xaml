	<SolidColorBrush x:Key="stdTextBlockForeGround">White</SolidColorBrush>

	<SolidColorBrush x:Key="stdForeGround">White</SolidColorBrush>
	<SolidColorBrush x:Key="stdBackGround">#FF262E34</SolidColorBrush>
	<SolidColorBrush x:Key="stdMouseOver">#FF455158</SolidColorBrush>
