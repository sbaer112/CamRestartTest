	<CornerRadius x:Key="stdCornerRadius">1</CornerRadius>
	<Thickness x:Key="stdBorderThickness">1</Thickness>

	<Style TargetType="{x:Type Label}">
		<Setter Property="Foreground" Value="{StaticResource stdTextBlockForeGround}"/>
		<Setter Property="FontSize" Value="15" />
	</Style>

