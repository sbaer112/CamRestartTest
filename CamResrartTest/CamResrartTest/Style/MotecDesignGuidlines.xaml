		<Setter Property="ToolTip" Value="Show more details"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type CheckBox}">
					<Grid Background="Transparent">
						<Viewbox Height="20">
							<Path Name="icon" Stroke="{TemplateBinding Foreground}" StrokeThickness="2" Fill="{TemplateBinding Foreground}"  Data="M 9 4 C 6.2504839 4 4 6.2504839 4 9 L 4 41 C 4 43.749516 6.2504839 46 9 46 L 41 46 C 43.749516 46 46 43.749516 46 41 L 46 9 C 46 6.2504839 43.749516 4 41 4 L 9 4 z M 9 6 L 41 6 C 42.668484 6 44 7.3315161 44 9 L 44 41 C 44 42.668484 42.668484 44 41 44 L 9 44 C 7.3315161 44 6 42.668484 6 41 L 6 9 C 6 7.3315161 7.3315161 6 9 6 z M 19.990234 11.990234 A 1.0001 1.0001 0 0 0 19.292969 13.707031 L 30.585938 25 L 19.292969 36.292969 A 1.0001 1.0001 0 1 0 20.707031 37.707031 L 33.414062 25 L 20.707031 12.292969 A 1.0001 1.0001 0 0 0 19.990234 11.990234 z"></Path>
						</Viewbox>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="Foreground" Value="{StaticResource stdBlue}" />
						</Trigger>
						
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Foreground" Value="{StaticResource stdClick}" />
							<Setter Property="Background" Value="{StaticResource stdDisabled}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
